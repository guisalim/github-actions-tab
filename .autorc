{
    "plugins": [
        "npm",
        "all-contributors",
        "conventional-commits",
        "first-time-contributor",
        "released"
    ],
    "onlyPublishWithReleaseLabel": true,
    "prereleaseBranches": [
        "release/v1",
        "release/v2"
    ],
    "release": {
        "prerelease": true
    }
}